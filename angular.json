      }
    },
    "@libs/game-lib": {
      "projectType": "library",
      "root": "@libs/game-lib",
      "sourceRoot": "@libs/game-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
