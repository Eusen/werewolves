{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "defaultProject": "@apps/werewolves",
  "newProjectRoot": "",
  "projects": {
    "@apps/werewolves": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "@apps/werewolves",
      "sourceRoot": "@apps/werewolves/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/@apps/werewolves",
            "index": "@apps/werewolves/src/index.html",
            "main": "@apps/werewolves/src/main.ts",
            "polyfills": "@apps/werewolves/src/polyfills.ts",
            "tsConfig": "@apps/werewolves/tsconfig.app.json",
            "aot": true,
            "assets": [
              "@apps/werewolves/src/favicon.ico",
              "@apps/werewolves/src/assets"
            ],
            "styles": [
              "@apps/werewolves/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "@apps/werewolves/src/environments/environment.ts",
                  "with": "@apps/werewolves/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "@apps/werewolves:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "@apps/werewolves:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "@apps/werewolves:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "@apps/werewolves/src/test.ts",
            "polyfills": "@apps/werewolves/src/polyfills.ts",
            "tsConfig": "@apps/werewolves/tsconfig.spec.json",
            "karmaConfig": "@apps/werewolves/karma.conf.js",
            "assets": [
              "@apps/werewolves/src/favicon.ico",
              "@apps/werewolves/src/assets"
            ],
            "styles": [
              "@apps/werewolves/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
                "@apps/werewolves/tsconfig.app.json",
                "@apps/werewolves/tsconfig.spec.json",
                "@apps/werewolves/e2e/tsconfig.json"
              ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
          "e2e": {
            "builder": "@angular-devkit/build-angular:protractor",
            "options": {
              "protractorConfig": "@apps/werewolves/e2e/protractor.conf.js",
              "devServerTarget": "@apps/werewolves:serve"
            },
            "configurations": {
              "production": {
                "devServerTarget": "@apps/werewolves:serve:production"
              }
            }
          }
      }
    },
    "@servers/main-server": {
      "projectType": "library",
      "root": "@servers/main-server",
      "sourceRoot": "@servers/main-server/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "@servers/main-server/tsconfig.lib.json",
            "project": "@servers/main-server/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "@servers/main-server/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "@servers/main-server/src/test.ts",
            "tsConfig": "@servers/main-server/tsconfig.spec.json",
            "karmaConfig": "@servers/main-server/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "@servers/main-server/tsconfig.lib.json",
              "@servers/main-server/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
